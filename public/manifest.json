{
  "manifest_version": 2,
  "name": "Bookmark Image Browser React",
  "version": "0.2.0",
  "description": "Browse through your bookmarked images.",
  "icons": {
    "16": "Logo_16.png",
    "48": "Logo_48.png",
    "128": "Logo_128.png"
  },
  "options_page": "index.html",
  "permissions": [
    "bookmarks",
    "webRequest",
    "webRequestBlocking",
    "*://*.deviantart.com/",
    "*://*.pixiv.net/", "*://i.pximg.net/",
    "*://*.igeturl.com/"
  ],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk=' https://backend.deviantart.com; object-src 'self'"
}